model User {
    id          String       @id @default(cuid())
    name        String
    email       String       @unique
    contact     String?
    password    String
    videos      Video[]
    engagements Engagement[]
    followedBy  Follows[]    @relation("followedBy")
    following   Follows[]    @relation("following")

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    comments Comment[]
}
